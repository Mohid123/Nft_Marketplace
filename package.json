    "ng-otp-input": "^1.8.5",
    "ng-qrcode": "^5.2.0",
    "ng2-charts": "^2.3.2",
    "ng2-tel-input": "^2.3.0",
    "ngx-image-cropper": "^5.0.1",
    "ngx-intl-tel-input": "^3.2.0",
    "ngx-spinner": "^12.0.0",
    "ngx-toastr": "^14.1.4",
    "replace-in-file": "^6.3.2",
