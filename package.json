    "@cypress/webpack-preprocessor": "5.9.1",
    "@types/jasmine": "3.9.1",
    "@types/jest": "27.0.1",
    "@types/node": "15.6.0",
    "@typescript-eslint/eslint-plugin": "4.31.1",
